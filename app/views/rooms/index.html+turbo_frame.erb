<% if @events %>
  <%= turbo_frame_tag 'page_handler' do %>
    <%= turbo_stream_action_tag(
          'prepend',
          target: 'messages-container',
          template: render(@events).to_s
        ) %>
    <%= turbo_stream_action_tag(
          'replace',
          target: 'pager',
          template: render('room_events/pager', pagy: @pagy).to_s
        ) %>
  <% end %>
<% end %>
